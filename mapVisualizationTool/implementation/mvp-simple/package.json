  "devDependencies": {
    "@types/node": "=20.11.24",
    "@types/react": "=19.0.10",
    "@types/react-dom": "=19.0.4",
    "@vitejs/plugin-react": "=4.3.4",
    "autoprefixer": "=10.4.20",
    "postcss": "=8.5.3",
    "tailwindcss": "=3.4.1",
    "typescript": "=5.8.2",
    "vite": "=6.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
